{
    "name": "martin3zra/stocks-tracker",
    "type": "project",
    "description": "Lightweight REST API using Slim Framework to check on stocks.",
    "keywords": ["slim", "framework", "stocks"],
    "require": {
        "slim/slim": "4.*",
        "slim/psr7": "^1.7",
        "selective/basepath": "^2.2",
        "illuminate/database": "^11.10",
        "symfony/console": "^7.1",
        "php-di/php-di": "^7.0",
        "justinrainbow/json-schema": "^5.2",
        "firebase/php-jwt": "^6.10",
        "guzzlehttp/guzzle": "^7.8",
        "symfony/dotenv": "^7.1",
        "symfony/mailer": "^7.1",
        "symfony/mime": "^7.1",
        "php-amqplib/php-amqplib": "^3.6"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.2"
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    }
}
